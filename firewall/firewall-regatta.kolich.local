#!/bin/bash

IPT="/sbin/iptables"

UNIVERSE="0.0.0.0/0"
LOCALAREA="1.0.00/16"

WANIP="50.136.240.128"
ROUTERIP="1.0.0.2"
WEBSERVERIP="1.0.0.8"

LOOPBACK="lo"

## Comcast (internet), DHCP assigned IP-address
EXTIF="eth1"

## Intranet, statically signed IP-address
INTIF="eth2"

## --------------------------------------------------------------------------------------
## --------------------------------------------------------------------------------------

## Flush everything so we can start fresh.
$IPT -F
$IPT -F -t nat
$IPT --delete-chain

## --------------------------------------------------------------------------------------
## --------------------------------------------------------------------------------------

## BASIC minimal NAT configuration.  Not used, just here for documentation purposes.
#$IPT -A FORWARD -i eth2 -o eth1 -p all -m state --state NEW,RELATED,ESTABLISHED -j ACCEPT
#$IPT -A FORWARD -i eth1 -o eth2 -p all -m state --state RELATED,ESTABLISHED -j ACCEPT
#$IPT -A POSTROUTING -t nat -o eth1 -j MASQUERADE

## Forward all packets from eth1 (internal network) to eth0 (the internet).
$IPT -A FORWARD -i $INTIF -o $EXTIF -m state --state NEW,RELATED,ESTABLISHED -j ACCEPT
## Forward packets that are part of existing and related connections
## from EXTERNAL to INTERNAL.
$IPT -A FORWARD -i $EXTIF -o $INTIF -m state --state RELATED,ESTABLISHED -j ACCEPT
## Permit packets into firewall itself that are part of existing and
## related connections on the external interface.
$IPT -A INPUT -i $EXTIF -m state --state RELATED,ESTABLISHED -j ACCEPT
## Enable masquerade.
$IPT -A POSTROUTING -t nat -o $EXTIF -j MASQUERADE

## Redirect all NAT'ed packets bound for destination port 80 to localhost:8088
## (a transparent proxy running on the localhost, the gateway). This triggers outgoing
## HTTP port 80 to be funneled through a transparent proxy running on port 8088
## of the gateway/localhost. NOTE: The REDIRECT target _always_ redirects the packet
## to the machine itself by changing the destination IP to the primary address of the
## incoming interface (locally-generated packets are mapped to the 127.0.0.1 address).
## E.g., always redirect's back to the local machine (that's why there's no IP-address
## specified for the destination).
#$IPT -A PREROUTING -i $INTIF -t nat -p tcp --dport 80 -j REDIRECT --to-ports 8088

## --------------------------------------------------------------------------------------
## --------------------------------------------------------------------------------------

## Allow all inputs to firewall/gateway from the internal network only
## on specific ports.
## SSH
$IPT -A INPUT -i $INTIF -s $LOCALAREA -p tcp --dport 22 -j ACCEPT
## DNS -- Note, I was originally only allowing in DNS UDP, but then
## I was doing some external lookups on some names and was seeing a
## ";; Truncated, retrying in TCP mode." from nslookup so I had to
## also allow in TCP lookups too for things to work properly.
$IPT -A INPUT -i $INTIF -s $LOCALAREA -p udp --dport 53 -j ACCEPT
$IPT -A INPUT -i $INTIF -s $LOCALAREA -p tcp --dport 53 -j ACCEPT
## HTTP and HTTPS
$IPT -A INPUT -i $INTIF -s $LOCALAREA -p tcp --dport 80 -j ACCEPT
$IPT -A INPUT -i $INTIF -s $LOCALAREA -p tcp --dport 443 -j ACCEPT
$IPT -A INPUT -i $INTIF -s $LOCALAREA -p tcp --dport 8080 -j ACCEPT
## Local web-proxy
$IPT -A INPUT -i $INTIF -s $LOCALAREA -p tcp --dport 8088 -j ACCEPT

## Allow pings on the internal interface as well (we need to check
## if networking on the gateway/firewally host is up).
$IPT -A INPUT -i $INTIF -s $LOCALAREA -p icmp -j ACCEPT

## IMPORTANT: Accept everything else on the internal interface that's
## related or previously established.  This lets the localhost
## communicate with VM's and other services running locally on
## the gateway/firewall directly.
$IPT -A INPUT -i $INTIF -m state --state RELATED,ESTABLISHED -j ACCEPT

## Loopback allows everything in, from everywhere.
$IPT -A INPUT -i $LOOPBACK -s $UNIVERSE -d $UNIVERSE -j ACCEPT

## --------------------------------------------------------------------------------------
## --------------------------------------------------------------------------------------

## Deny any packet coming in on the public internet interface
## which has a spoofed source address from our local network.
$IPT -A INPUT -i $EXTIF -s $LOCALAREA -j DROP
$IPT -A INPUT -i $EXTIF -s 127.0.0.0/8 -j DROP

