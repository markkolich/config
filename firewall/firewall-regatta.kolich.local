## Security hardening
$IPT -A INPUT -i $EXTIF -p tcp -m tcp --tcp-flags RST RST -m limit --limit 2/second --limit-burst 2 -j ACCEPT
$IPT -A INPUT -i $EXTIF -p tcp --syn -j DROP
$IPT -A INPUT -i $EXTIF -p tcp --tcp-flags ALL FIN,URG,PSH -j DROP
$IPT -A INPUT -i $EXTIF -p tcp --tcp-flags ALL ALL -j DROP
$IPT -A INPUT -i $EXTIF -p tcp --tcp-flags ALL NONE -j DROP
$IPT -A INPUT -i $EXTIF -p tcp --tcp-flags ALL FIN -j DROP
$IPT -A INPUT -i $EXTIF -p tcp --tcp-flags ALL FIN,PSH,URG -j DROP
$IPT -A INPUT -i $EXTIF -p tcp --tcp-flags SYN,RST SYN,RST -j DROP
$IPT -A INPUT -i $EXTIF -p tcp --tcp-flags FIN,ACK FIN -j DROP
$IPT -A INPUT -i $EXTIF -p tcp --tcp-flags ALL SYN,RST,ACK,FIN,URG -j DROP
$IPT -A INPUT -i $EXTIF -p tcp ! --syn -m state --state NEW -j DROP
$IPT -A INPUT -i $EXTIF -f -j DROP

## If we want to allow pings on the external facing interface,
## uncomment the line below.
#$IPT -A INPUT -i $EXTIF -p icmp -j ACCEPT

## Logging
#$IPT -A INPUT -i $INTIF -j LOG --log-ip-options --log-level 5 --log-prefix "INPUT "
#$IPT -A OUTPUT -o eth0 -j LOG --log-ip-options --log-level 5 --log-prefix "OUTPUT "
#$IPT -A FORWARD -j LOG --log-ip-options --log-level 5 --log-prefix "FORWARD "
#$IPT -A PREROUTING -t nat -j LOG --log-ip-options --log-prefix "PREROUTING "
#$IPT -A POSTROUTING -t nat -j LOG --log-ip-options --log-level 5

## Drop everything else, regardless of port or protocol on the
## the external interface.
$IPT -A FORWARD -i $EXTIF -j DROP
$IPT -A INPUT -i $EXTIF -j DROP

## Useful for logging to see what's dropped in 'dmesg'
## from the internal interface.
#$IPT -i $INTIF -I INPUT 5 -j LOG --log-prefix "iptables denied: " --log-level 7

## Drop everything else, regardless of the port of protocol on
## the internal interface.
$IPT -A FORWARD -i $INTIF -j DROP
$IPT -A INPUT -i $INTIF -j DROP

