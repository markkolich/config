$TTL	1h
; private zone master file used by stealth server(s)
; provides public and private services and hosts
kolich.local.  IN      SOA   regatta.kolich.local. support.kolich.com. (
                              2010031500 ; se = serial number
                              3h         ; ref = refresh
                              15m        ; ret = update retry
                              3w         ; ex = expiry
                              3h         ; min = minimum
                              )
              IN      NS      regatta.kolich.local.
; so localhost resolves correctly
localhost	IN	A	127.0.0.1
; statically assigned hosts
fiji		IN      A       1.0.0.1
regatta		IN	A	1.0.0.2
procurve	IN	A	1.0.0.5
axis		IN	A	1.0.0.6
hpclj		IN	A	1.0.0.7
; Non-core VM's
xp		IN	A	1.0.0.10
service		IN	A	1.0.0.11
zimbra		IN	A	1.0.0.12
optimally	IN	A	1.0.0.13
ssh		IN	A	1.0.0.14
; The wireless gateway has its own "fixed" IP-address
; assigned statically by the DHCP server.
dhcp-fiji	IN	A	1.0.0.100
